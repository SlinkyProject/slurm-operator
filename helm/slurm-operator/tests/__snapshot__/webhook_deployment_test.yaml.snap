                  port: 8081
              volumeMounts:
                - mountPath: /tmp/k8s-webhook-server/serving-certs/
                  name: certificates
                  readOnly: true
          hostname: slurm-operator-webhook
          priorityClassName: null
          serviceAccountName: slurm-operator-webhook
          volumes:
            - name: certificates
              secret:
                defaultMode: 420
